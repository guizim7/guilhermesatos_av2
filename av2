// 10, 11 e 12
var media, nota1, nota2, nota3;

for(i=0; i<=2; i++){
  nota1= Number(prompt ("Digite sua primeira nota:"));
  nota2= Number(prompt ("Digite sua segunda nota:"));
  nota3= Number(prompt ("Digite sua terceira nota:")); 
  media = (nota1 + nota2 + nota3)/3; 

  alert("sua média é "+media)
  
  frequencia= prompt("Digite sua frequencia");

  if ( frequencia >= 75 && media >= 60) {
  alert("aprovado");
    } 
  else{
    alert("reprovado");
  }
}
